
<%- include("partials/header") -%>

<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label for="newTitle">Title</label>
    <input type="text" class="form-control" name="newTitle" autocomplete="off"><br>
    <label for="newBlog">Post</label>
    <textarea name="newBlog" class="form-control" rows="5"></textarea>
